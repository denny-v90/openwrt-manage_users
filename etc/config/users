
config user 'tech'
	option username 'tech'
	option password 'tech'
	list subnode 'status/Processes'
	list subnode 'network/Hostnames'
	list subnode 'network/Firewall'
	list subnode 'system/System'
	list subnode 'system/Administration'
	list subnode 'system/Manage Users'
	list subnode 'system/Software'
	list subnode 'system/Startup'
	list subnode 'system/Scheduled Tasks'

config user 'user'
	option username 'user'
	option password 'user'
	list node 'network'
	list subnode 'status/Firewall'
	list subnode 'status/Routes'
	list subnode 'status/System Log'
	list subnode 'status/Kernel Log'
	list subnode 'status/Processes'
	list subnode 'system/System'
	list subnode 'system/Administration'
	list subnode 'system/Manage Users'
	list subnode 'system/Software'
	list subnode 'system/Startup'
	list subnode 'system/Scheduled Tasks'
	list subnode 'system/Backup / Flash Firmware'

